{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "User created event",
    "type": "object",
    "properties": {
        "event_id": {
            "type": "string"
        },
        "event_version": {
            "type": "string"
        },
        "event_name": {
            "type": "string"
        },
        "event_time": {
            "type": "string"
        },
        "producer": {
            "type": "string"
        },
        "data": {
            "type": "object",
            "properties": {
                "publicId": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "role": {
                    "type": "string"
                }
            },
            "required": [
                "publicId",
                "email",
                "name",
                "role"
            ]
        }
    },
    "required": [
        "event_id",
        "event_version",
        "event_name",
        "event_time",
        "producer",
        "data"
    ]
}

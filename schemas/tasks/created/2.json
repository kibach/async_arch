{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Generated schema for Root",
    "type": "object",
    "properties": {
        "event_id": {
            "type": "string",
            "format": "uuid"
        },
        "event_version": {
            "type": "string"
        },
        "event_name": {
            "type": "string"
        },
        "event_time": {
            "type": "string",
            "format": "date-time"
        },
        "producer": {
            "type": "string"
        },
        "data": {
            "type": "object",
            "properties": {
                "publicId": {
                    "type": "string"
                },
                "title": {
                    "type": "string",
                    "pattern": "^[^\\[\\]]+$"
                },
                "jira_id": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "assigneePublicId": {
                    "type": "string"
                },
                "assignationFee": {
                    "type": "number"
                },
                "completionReward": {
                    "type": "number"
                }
            },
            "required": [
                "publicId",
                "title",
                "description",
                "assigneePublicId",
                "assignationFee",
                "completionReward"
            ]
        }
    },
    "required": [
        "event_id",
        "event_version",
        "event_name",
        "event_time",
        "producer",
        "data"
    ]
}